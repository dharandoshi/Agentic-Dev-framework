{
  "project_templates": {
    "web_application": {
      "name": "Full-Stack Web Application",
      "description": "Complete web application with frontend, backend, and database",
      "agents_required": [
        "requirements-analyst",
        "system-architect",
        "senior-backend-engineer",
        "senior-frontend-engineer",
        "qa-engineer",
        "devops-engineer"
      ],
      "workflow_phases": [
        {
          "phase": "Planning",
          "duration": "1-2 days",
          "agents": ["requirements-analyst", "scrum-master"],
          "deliverables": ["requirements.md", "sprint-plan.md", "user-stories.md"]
        },
        {
          "phase": "Architecture",
          "duration": "2-3 days",
          "agents": ["system-architect", "tech-lead"],
          "deliverables": ["architecture.md", "database-schema.sql", "api-spec.yaml"]
        },
        {
          "phase": "Backend Development",
          "duration": "5-7 days",
          "agents": ["senior-backend-engineer", "data-engineer"],
          "deliverables": ["api/", "models/", "services/", "tests/backend/"]
        },
        {
          "phase": "Frontend Development",
          "duration": "5-7 days",
          "agents": ["senior-frontend-engineer"],
          "deliverables": ["src/components/", "src/pages/", "src/services/", "tests/frontend/"]
        },
        {
          "phase": "Integration",
          "duration": "2-3 days",
          "agents": ["integration-engineer", "senior-backend-engineer", "senior-frontend-engineer"],
          "deliverables": ["integration-tests/", "e2e-tests/"]
        },
        {
          "phase": "Testing",
          "duration": "3-4 days",
          "agents": ["qa-engineer"],
          "deliverables": ["test-plan.md", "test-cases/", "bug-reports/", "test-results.md"]
        },
        {
          "phase": "Deployment",
          "duration": "1-2 days",
          "agents": ["devops-engineer", "cloud-architect"],
          "deliverables": ["Dockerfile", "docker-compose.yml", ".github/workflows/", "kubernetes/"]
        },
        {
          "phase": "Documentation",
          "duration": "2-3 days",
          "agents": ["technical-writer"],
          "deliverables": ["README.md", "docs/api/", "docs/user-guide/", "docs/deployment/"]
        }
      ],
      "tech_stack": {
        "frontend": ["React", "Vue", "Angular", "Next.js"],
        "backend": ["Node.js", "Python/Django", "Ruby on Rails", "Go"],
        "database": ["PostgreSQL", "MongoDB", "MySQL", "Redis"],
        "deployment": ["Docker", "Kubernetes", "AWS", "Vercel"]
      },
      "project_structure": {
        "root": {
          "frontend/": "Frontend application",
          "backend/": "Backend API",
          "database/": "Database schemas and migrations",
          "docs/": "Documentation",
          "tests/": "Test suites",
          "scripts/": "Utility scripts",
          ".github/": "CI/CD workflows",
          "docker/": "Docker configurations"
        }
      },
      "commands": [
        "Act as requirements-analyst and gather requirements for {project_name}",
        "Act as system-architect and design the architecture",
        "Act as senior-backend-engineer and implement the API",
        "Act as senior-frontend-engineer and build the UI",
        "Act as qa-engineer and test the application",
        "Act as devops-engineer and deploy to production"
      ]
    },
    
    "api_service": {
      "name": "RESTful API Service",
      "description": "Backend API service with authentication and database",
      "agents_required": [
        "system-architect",
        "senior-backend-engineer",
        "security-engineer",
        "qa-engineer",
        "devops-engineer"
      ],
      "workflow_phases": [
        {
          "phase": "API Design",
          "duration": "1-2 days",
          "agents": ["system-architect"],
          "deliverables": ["openapi.yaml", "data-models.md"]
        },
        {
          "phase": "Implementation",
          "duration": "4-5 days",
          "agents": ["senior-backend-engineer"],
          "deliverables": ["src/routes/", "src/controllers/", "src/models/", "src/middleware/"]
        },
        {
          "phase": "Security",
          "duration": "1-2 days",
          "agents": ["security-engineer"],
          "deliverables": ["auth/", "security-audit.md"]
        },
        {
          "phase": "Testing",
          "duration": "2-3 days",
          "agents": ["qa-engineer"],
          "deliverables": ["tests/unit/", "tests/integration/", "postman-collection.json"]
        },
        {
          "phase": "Deployment",
          "duration": "1 day",
          "agents": ["devops-engineer"],
          "deliverables": ["Dockerfile", "helm-charts/", "ci-cd.yml"]
        }
      ],
      "tech_stack": {
        "languages": ["Node.js/Express", "Python/FastAPI", "Go/Gin", "Java/Spring"],
        "database": ["PostgreSQL", "MongoDB", "DynamoDB"],
        "auth": ["JWT", "OAuth2", "Auth0"],
        "deployment": ["Docker", "Kubernetes", "AWS Lambda"]
      },
      "project_structure": {
        "root": {
          "src/": "Source code",
          "tests/": "Test files",
          "docs/": "API documentation",
          "config/": "Configuration files",
          "scripts/": "Utility scripts"
        }
      },
      "commands": [
        "Act as system-architect and design REST API for {feature}",
        "Act as senior-backend-engineer and implement {endpoint}",
        "Act as security-engineer and add authentication",
        "Act as qa-engineer and test the API endpoints"
      ]
    },
    
    "microservices": {
      "name": "Microservices Architecture",
      "description": "Distributed microservices system with service mesh",
      "agents_required": [
        "system-architect",
        "senior-backend-engineer",
        "cloud-architect",
        "devops-engineer",
        "sre-engineer"
      ],
      "workflow_phases": [
        {
          "phase": "Architecture",
          "duration": "2-3 days",
          "agents": ["system-architect", "cloud-architect"],
          "deliverables": ["architecture.md", "service-map.md", "communication-patterns.md"]
        },
        {
          "phase": "Service Development",
          "duration": "7-10 days",
          "agents": ["senior-backend-engineer"],
          "deliverables": ["services/", "shared-libs/", "proto/"]
        },
        {
          "phase": "Service Mesh",
          "duration": "2-3 days",
          "agents": ["devops-engineer", "cloud-architect"],
          "deliverables": ["istio/", "consul/", "service-mesh-config/"]
        },
        {
          "phase": "Orchestration",
          "duration": "2-3 days",
          "agents": ["devops-engineer"],
          "deliverables": ["kubernetes/", "helm/", "terraform/"]
        },
        {
          "phase": "Monitoring",
          "duration": "1-2 days",
          "agents": ["sre-engineer"],
          "deliverables": ["monitoring/", "alerts/", "dashboards/"]
        }
      ],
      "tech_stack": {
        "services": ["Go", "Node.js", "Python", "Java"],
        "communication": ["gRPC", "REST", "GraphQL", "Message Queue"],
        "service_mesh": ["Istio", "Linkerd", "Consul"],
        "orchestration": ["Kubernetes", "Docker Swarm"],
        "monitoring": ["Prometheus", "Grafana", "Jaeger", "ELK Stack"]
      },
      "project_structure": {
        "root": {
          "services/": "Individual microservices",
          "api-gateway/": "API gateway service",
          "shared/": "Shared libraries",
          "infrastructure/": "Infrastructure as code",
          "monitoring/": "Monitoring configuration"
        }
      },
      "commands": [
        "Act as system-architect and design microservices architecture",
        "Act as senior-backend-engineer and implement {service_name} service",
        "Act as cloud-architect and design service mesh",
        "Act as devops-engineer and set up Kubernetes cluster",
        "Act as sre-engineer and implement monitoring"
      ]
    },
    
