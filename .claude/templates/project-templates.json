      },
      "project_structure": {
        "root": {
          "mobile/": "Mobile app source",
          "backend/": "Backend API",
          "shared/": "Shared types/utils",
          "assets/": "Images and resources"
        }
      },
      "commands": [
        "Act as requirements-analyst and define mobile app requirements",
        "Act as senior-frontend-engineer and build React Native app",
        "Act as senior-backend-engineer and create mobile backend",
        "Act as qa-engineer and test on iOS and Android"
      ]
